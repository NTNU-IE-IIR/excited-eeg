<?xml version="1.0" encoding="utf-8"?>

<android.support.percent.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ListView
        android:id="@+id/historylist"
        style="@style/MobileHistoryList" />

    <TableLayout
        android:id="@+id/mobileTableLayout"
        style="@style/MobileTableStyle">

        <TableRow style="@style/MobileRowStyle">

            <LinearLayout
                style="@style/MobileGroupStyle"
                android:weightSum="3">

                <TextView
                    android:id="@+id/sym_e"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_t"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_a"
                    style="@style/DefaultTextViewLetterStyle" />
            </LinearLayout>

            <LinearLayout
                style="@style/MobileGroupStyle"
                android:weightSum="3">

                <TextView
                    android:id="@+id/sym_o"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_i"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_n"
                    style="@style/DefaultTextViewLetterStyle" />
            </LinearLayout>

            <LinearLayout
                style="@style/MobileGroupStyle"
                android:weightSum="3">

                <include layout="@layout/layout_mobile_deletion_utilities" />

            </LinearLayout>


        </TableRow>

        <TableRow style="@style/MobileRowStyle">

            <LinearLayout
                style="@style/MobileGroupStyle"
                android:weightSum="3">

                <TextView
                    android:id="@+id/sym_s"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_r"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_h"
                    style="@style/DefaultTextViewLetterStyle" />
            </LinearLayout>


            <LinearLayout
                style="@style/MobileGroupStyle"
                android:weightSum="4">

                <TextView
                    android:id="@+id/sym_l"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_d"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_c"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_u"
                    style="@style/DefaultTextViewLetterStyle" />
            </LinearLayout>

            <LinearLayout
                style="@style/MobileGroupStyle"
                android:weightSum="4">

                <TextView
                    android:id="@+id/sym_w"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_y"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_b"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_v"
                    style="@style/DefaultTextViewLetterStyle" />
            </LinearLayout>

        </TableRow>

        <TableRow style="@style/MobileRowStyle">


            <LinearLayout
                style="@style/MobileGroupStyle"
                android:weightSum="4">

                <TextView
                    android:id="@+id/sym_m"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_f"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_p"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_g"
                    style="@style/DefaultTextViewLetterStyle" />
            </LinearLayout>

            <LinearLayout
                style="@style/MobileGroupStyle"
                android:weightSum="5">

                <TextView
                    android:id="@+id/sym_k"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_x"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_j"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_q"
                    style="@style/DefaultTextViewLetterStyle" />

                <TextView
                    android:id="@+id/sym_z"
                    style="@style/DefaultTextViewLetterStyle" />

            </LinearLayout>

            <include layout="@layout/layout_mobile_dictionary_utilities" />


        </TableRow>

        <TableRow style="@style/MobileRowStyle">

            <include layout="@layout/layout_mobile_bottom_row" />

        </TableRow>
    </TableLayout>

    <no.ntnu.stud.avikeyb.gui.utils.AutoScrollListView
        android:id="@+id/autolistview"
        style="@style/DictionaryListStyle"
        android:layout_toEndOf="@+id/mobileTableLayout" />

    <TextView android:id="@+id/emptyInfo"
        android:layout_toEndOf="@+id/mobileTableLayout"
        style="@style/WaitingListTextViewStyle"/>

</android.support.percent.PercentRelativeLayout>
